{
  "name": "@bvetree/parser",
  "version": "0.0.1",
  "description": "Parser for BveTrainSim 5/6 file format",
  "main": "dist/index.js",
  "files": [
    "dist",
    "!**/tests"
  ],
  "scripts": {
    "postinstall": "npm run gen-v2map",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src",
    "gen-v2map": "antlr4ts -Xexact-output-dir -o src/map-v2/gen -visitor src/map-v2/definition/MapParser.g4 src/map-v2/definition/MapLexer.g4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aoisupersix/bvetree.git"
  },
  "keywords": [
    "javascript",
    "bve"
  ],
  "author": "aoisupersix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aoisupersix/bvetree/issues"
  },
  "homepage": "https://github.com/aoisupersix/bvetree#readme",
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "typescript": "^4.6.3"
  }
}
